  "generators": {
    "data-access": {
      "factory": "./src/generators/data-access/generator",
      "schema": "./src/generators/data-access/schema.json",
      "description": "data-access generator"
    },
    "core": {
      "factory": "./src/generators/core/generator",
      "schema": "./src/generators/core/schema.json",
