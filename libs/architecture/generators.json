    "feature": {
      "factory": "./src/generators/feature/generator",
      "schema": "./src/generators/feature/schema.json",
      "description": "feature generator"
    },
    "ui": {
      "factory": "./src/generators/ui/generator",
      "schema": "./src/generators/ui/schema.json",
      "description": "ui generator"
    }
  }
}
