{
  "name": "shared-avatar-images-features-avatar",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/avatar-images/features/avatar/src",
  "prefix": "feature-avatar",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/avatar-images/features/avatar/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/avatar-images/features/avatar/**/*.ts",
          "libs/shared/avatar-images/features/avatar/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:feature"]
}
