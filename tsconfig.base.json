      "@kbru/battle-companion/features/roster-list": [
        "libs/battle-companion/features/roster-list/src/index.ts"
      ],
      "@kbru/battle-companion/features/start-page": [
        "libs/battle-companion/features/start-page/src/index.ts"
      ],
      "@kbru/shared/ionic/ui/file-input": [
        "libs/shared/ionic/ui/file-input/src/index.ts"
      ],
      "@kbru/shared/utils/architecture": [
        "libs/shared/utils/architecture/src/index.ts"
      ],
      "@kbru/shared/utils/array-utils": [
        "libs/shared/utils/array-utils/src/index.ts"
      ],
      "@kbru/shared/utils/ngrx-architecture": [
